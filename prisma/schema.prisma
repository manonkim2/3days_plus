generator client {
  provider = "prisma-client-js"
  output   = "./client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @unique
  name       String
  email      String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  image_url  String?
  social     String?
}
